      <div class="row">
        <div class="col-lg-12">
          <div class="page-header">
            <h1>Edit Room</h1>
          </div>
          <%= render 'form', room: @room %>
        </div>
      </div>
    </div>

    <%= link_to 'Cancel', rooms_path, class: "btn btn-primary" %>
<% end %>
